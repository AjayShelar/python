    "def reverse1(string):\n",
    "    li = []\n",
    "    li.extend(string)\n",
    "    print(li)\n",
    "    size = len(li)\n",
    "    for i in range(0, size//2):\n",
    "        li[i], li[size-1-i] = li[size-1-i], li[i]\n",
    "    return li\n",
    "    \n",
    "#driver code\n",
    "string = \"ajay\"\n",
    "reverse1(string)\n",
    "\n"
